{
	"name": "fractils",
	"description": "some stuff I often use",
	"version": "3.1.2",
	"author": "Braden Wiggins",
	"license": "MIT",
	"type": "module",
	"keywords": [
		"fractils",
		"utils",
		"svelte",
		"typescript",
		"javascript"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fractalhq/fractils.git"
	},
	"bugs": {
		"url": "https://github.com/fractalhq/fractils/issues"
	},
	"homepage": "https://github.com/fractalhq/fractils#readme",
	"scripts": {
		"dev": "vite dev",
		"test": "vitest",
		"debug": "NODE_OPTIONS=\"--inspect\" vite dev",
		"build": "vite-node ./src/shiki/syntax-highlighter.ts && rm -rf dist && vite build",
		"preview": "vite preview",
		"package": "svelte-package",
		"release": "pnpm package && changeset publish",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"dependencies": {
		"esm-env": "^1.0.0"
	},
	"peerDependencies": {
		"svelte": "^3.59.1"
	},
	"devDependencies": {
		"@changesets/cli": "^2.26.2",
		"@microsoft/api-extractor": "^7.38.0",
		"@microsoft/tsdoc": "^0.14.2",
		"@sveltejs/adapter-vercel": "3.0.3",
		"@sveltejs/kit": "1.25.1",
		"@sveltejs/package": "2.2.2",
		"@svitejs/changesets-changelog-github-compact": "^1.1.0",
		"@types/node": "^20.8.2",
		"@types/prismjs": "^1.26.1",
		"@vitest/ui": "^0.34.6",
		"chalk": "^5.3.0",
		"colors": "^1.4.0",
		"globby": "^13.2.2",
		"greset": "2.0.0",
		"highlight.js": "^11.8.0",
		"nanoid": "^5.0.1",
		"nodemon": "^3.0.1",
		"npm-run-all": "^4.1.5",
		"prettier": "3.0.3",
		"prismjs": "^1.29.0",
		"pug": "^3.0.2",
		"sass": "^1.68.0",
		"shiki": "^0.14.4",
		"shiki-twoslash": "^3.1.2",
		"svelte-check": "^3.5.2",
		"svelte-preprocess": "^5.0.4",
		"svelte2tsx": "^0.6.22",
		"tippy.js": "^6.3.7",
		"tslib": "^2.6.2",
		"tsutils": "^3.21.0",
		"typescript": "^5.2.2",
		"vite": "^4.4.10",
		"vite-node": "^0.34.6",
		"vitest": "^0.34.6"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"import": "./dist/index.js"
		},
		"./*": {
			"types": "./dist/*.d.ts",
			"svelte": "./dist/*.js",
			"import": "./dist/*.js"
		}
	},
	"types": "./dist/index.d.ts",
	"svelte": "./dist/index.js",
	"files": [
		"dist",
		"LICENSE"
	]
}