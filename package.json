{
	"name": "fractils",
	"description": "some stuff I often use",
	"version": "3.1.2",
	"author": "Braden Wiggins",
	"license": "MIT",
	"type": "module",
	"keywords": [
		"utils",
		"svelte",
		"sveltekit",
		"typescript",
		"javascript"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fractalhq/fractils.git"
	},
	"bugs": {
		"url": "https://github.com/fractalhq/fractils/issues"
	},
	"homepage": "https://github.com/fractalhq/fractils#readme",
	"scripts": {
		"dev": "vite dev",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"debug": "NODE_OPTIONS=\"--inspect\" vite dev",
		"build": "vite-node ./src/scripts/shiki/syntax-highlighter.ts && rm -rf dist && vite build",
		"preview": "vite preview",
		"package": "svelte-package",
		"release": "pnpm package && changeset publish",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"generate:css": "bash src/scripts/generate-css.sh",
		"generate:shiki": "vite-node ./src/scripts/shiki/syntax-highlighter.ts",
		"generate:docs": "bun src/scripts/extractinator/src/exports/cli.ts extract src/lib src/docs",
		"generate:docs:watch": "bun --watch src/scripts/extractinator/src/exports/cli.ts extract src/lib src/docs",
		"generate:all": "run-p generate:docs generate:css generate:shiki"
	},
	"dependencies": {
		"esm-env": "^1.0.0"
	},
	"peerDependencies": {
		"svelte": "^4.2.1"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.1",
		"@fastify/deepmerge": "^1.3.0",
		"@microsoft/api-extractor": "^7.39.0",
		"@microsoft/tsdoc": "^0.14.2",
		"@microsoft/tsdoc-config": "^0.16.2",
		"@sveltejs/adapter-vercel": "4.0.3",
		"@sveltejs/kit": "2.0.6",
		"@sveltejs/package": "2.2.5",
		"@svitejs/changesets-changelog-github-compact": "^1.1.0",
		"@types/node": "^20.10.5",
		"@types/prismjs": "^1.26.3",
		"@vitest/ui": "^1.1.0",
		"bun": "^1.0.20",
		"bun-types": "^1.0.20",
		"chalk": "^5.3.0",
		"chokidar": "^3.5.3",
		"colors": "^1.4.0",
		"globby": "^14.0.0",
		"greset": "2.0.0",
		"highlight.js": "^11.9.0",
		"kleur": "^4.1.5",
		"marked": "^11.1.0",
		"marked-highlight": "^2.1.0",
		"micromark": "^4.0.0",
		"nanoid": "^5.0.4",
		"nodemon": "^3.0.2",
		"npm-run-all": "^4.1.5",
		"prettier": "3.1.1",
		"prismjs": "^1.29.0",
		"pug": "^3.0.2",
		"sade": "^1.8.1",
		"sass": "^1.69.5",
		"semver": "^7.5.4",
		"shiki": "^0.14.7",
		"shiki-twoslash": "^3.1.2",
		"shikiji": "^0.9.12",
		"shikiji-transformers": "^0.9.12",
		"shikiji-twoslash": "^0.9.12",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"svelte-preprocess": "^5.1.3",
		"svelte2tsx": "^0.6.27",
		"tiny-glob": "^0.2.9",
		"tippy.js": "^6.3.7",
		"ts-morph": "^21.0.1",
		"tslib": "^2.6.2",
		"tsutils": "^3.21.0",
		"typescript": "^5.3.3",
		"vite": "^5.0.10",
		"vite-node": "^1.1.0",
		"vitest": "^1.1.0"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"import": "./dist/index.js"
		},
		"./*": {
			"types": "./dist/*.d.ts",
			"svelte": "./dist/*.js",
			"import": "./dist/*.js"
		}
	},
	"types": "./dist/index.d.ts",
	"svelte": "./dist/index.js",
	"files": [
		"dist",
		"LICENSE"
	]
}
