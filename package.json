{
  "name": "fractils",
  "description": "some stuff I often use",
  "version": "3.1.3",
  "author": "Braden Wiggins",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "utils",
    "svelte",
    "sveltekit",
    "typescript",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fractalhq/fractils.git"
  },
  "bugs": {
    "url": "https://github.com/fractalhq/fractils/issues"
  },
  "homepage": "https://fractils.fractal-hq.com",
  "scripts": {
    "dev": "vite dev",
    "test": "vitest watch --disable-console-intercept",
    "test:ui": "vitest watch --ui --disable-console-intercept",
    "debug": "NODE_OPTIONS=\"--inspect\" vite dev",
    "build": "vite-node ./src/scripts/shiki/syntax-highlighter.ts && rm -rf dist && vite build",
    "preview": "vite preview",
    "package": "svelte-package",
    "release": "pnpm package && changeset publish",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "generate:css": "bash src/scripts/generate-css.sh",
    "generate:shiki": "vite-node ./src/scripts/shiki/syntax-highlighter.ts",
    "extractinator": "bun src/scripts/extractinator/src/exports/cli.ts extract src/lib src/docs/raw",
    "extractinator:watch": "bun --watch src/scripts/extractinator/src/exports/cli.ts extract src/lib src/docs/raw",
    "extractinator:test": "bun src/scripts/extractinator/src/exports/cli.ts extract src/lib/test src/docs/raw/test",
    "extractinator:test:watch": "bun --watch src/scripts/extractinator/src/exports/cli.ts extract src/lib/test src/docs/raw/test --verbose",
    "docinator": "bun run src/lib/utils/docinator.ts ./src/docs/raw ./src/docs/highlighted",
    "docinator:watch": "bun run --watch src/lib/utils/docinator.ts ./src/docs/raw ./src/docs/highlighted",
    "docinator:test": "bun run src/lib/utils/docinator.ts ./src/docs/raw/test ./src/docs/highlighted/test",
    "docinator:test:watch": "bun run --watch src/lib/utils/docinator.ts ./src/docs/raw/test ./src/docs/highlighted/test",
    "generate:docs": "pnpm extractinator && pnpm docinator",
    "generate:api": "pnpm package && api-extractor run --local",
    "generate:all": "run-s generate:docs generate:css generate:shiki generate:api"
  },
  "peerDependencies": {
    "svelte": "^4.2.10"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.2",
    "@fastify/deepmerge": "^1.3.0",
    "@formkit/auto-animate": "^0.8.2",
    "@microsoft/api-documenter": "^7.24.8",
    "@microsoft/api-extractor": "^7.43.7",
    "@microsoft/tsdoc": "^0.14.2",
    "@microsoft/tsdoc-config": "^0.16.2",
    "@nanostores/persistent": "^0.10.1",
    "@preact/signals": "^1.2.3",
    "@preact/signals-core": "^1.6.0",
    "@shikijs/transformers": "^1.5.2",
    "@sveltejs/adapter-vercel": "5.3.0",
    "@sveltejs/kit": "2.5.9",
    "@sveltejs/package": "2.3.1",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@svitejs/changesets-changelog-github-compact": "^1.1.0",
    "@types/cookie": "^0.6.0",
    "@types/node": "^20.12.12",
    "@types/prismjs": "^1.26.4",
    "@types/react": "^18.3.2",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "bun": "^1.1.8",
    "bun-types": "^1.1.8",
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "cli-highlight": "^2.1.11",
    "colors": "^1.4.0",
    "cookie": "^0.6.0",
    "csstype": "^3.1.3",
    "deep-object-diff": "^1.1.9",
    "errorstacks": "^2.4.1",
    "esm-env": "^1.0.0",
    "globby": "^14.0.1",
    "greset": "2.0.0",
    "highlight.js": "^11.9.0",
    "jsdom": "^24.0.0",
    "jsdom-testing-mocks": "^1.13.0",
    "json-schema-to-typescript": "^14.0.4",
    "kleur": "^4.1.5",
    "marked": "^12.0.2",
    "marked-highlight": "^2.1.1",
    "micromark": "^4.0.0",
    "nanostores": "^0.10.3",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "3.2.5",
    "prismjs": "^1.29.0",
    "pug": "^3.0.2",
    "react": "^18.3.1",
    "sade": "^1.8.1",
    "sass": "^1.77.2",
    "semver": "^7.6.2",
    "shiki": "^1.5.2",
    "shiki-twoslash": "^3.1.2",
    "shikiji": "0.10.2",
    "shikiji-transformers": "0.10.2",
    "shikiji-twoslash": "0.10.2",
    "skott": "^0.33.2",
    "svelte": "^4.2.17",
    "svelte-check": "^3.7.1",
    "svelte-preprocess": "^5.1.4",
    "svelte2tsx": "^0.7.8",
    "tiny-glob": "^0.2.9",
    "tippy.js": "^6.3.7",
    "ts-morph": "^22.0.0",
    "tslib": "^2.6.2",
    "tsutils": "^3.21.0",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-node": "^1.6.0",
    "vitest": "^1.6.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "svelte": "./dist/*.js",
      "import": "./dist/*.js"
    }
  },
  "types": "./dist/index.d.ts",
  "svelte": "./dist/index.js",
  "files": [
    "dist",
    "LICENSE"
  ]
}